\par \pard\plain \ltrpar\s1\cf1{\*\hyphen2\hyphlead2\hyphtrail2\hyphmax0}\aspalpha\ql\rtlch\afs24\lang255\ltrch\dbch\af8\langfe1033\hich\fs24\lang1033\loch\fs24\lang1033 
